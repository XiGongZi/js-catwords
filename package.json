    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "nodemon": "^2.0.3",
    "cross-env": "^5.2.0",
    "rimraf": "^2.6.3",
    "shx": "^0.3.2",
    "dotenv": "^8.2.0"
  },
